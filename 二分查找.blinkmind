      "collapse": false,
      "style": null,
      "blocks": [
        {
          "type": "CONTENT",
          "data": "int right_bound(int[] nums, int target) { \n   if (nums.length == 0) \n        return -1;\n   int left = 0, right = nums.length;\n   while (left < right) {    // 左闭右开区间\n        int mid = (left + right) / 2;\n        if (nums[mid] == target) {\n            left = mid + 1; // 注意，向右收缩\n        } else if (nums[mid] < target) {\n            left = mid + 1;\n        } else if (nums[mid] > target) {\n           right = mid; \n       }\n   }\n   return left - 1; // 注意\n}"
        }
      ]
